{
  "name": "pgbulk",
  "version": "0.4.1",
  "description": "A library for populate large amounts of data to PostgreSQL database in TypeScript",
  "main": "./build/index.js",
  "scripts": {
    "build": "tsc --build",
    "test": "docker compose up -d --wait && jest",
    "lint": "eslint --ignore-pattern build/**/*",
    "test:faker": "ts-node ./tests/populate.faker.ts",
    "prepare": "pnpm build"
  },
  "keywords": [
    "postgres",
    "bulk insert",
    "populate",
    "pgbulk"
  ],
  "files": [
    "build"
  ],
  "author": "Pedro Augusto <contato.pedrinfx@gmail.com>",
  "types": "build/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/peeeuzin/pgbulk.git"
  },
  "dependencies": {
    "csv-parser": "^3.2.0",
    "csv-stringify": "^6.5.2",
    "glob": "^11.0.1",
    "pg": "^8.13.1",
    "pg-copy-streams": "^6.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@faker-js/faker": "^9.4.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.0",
    "@types/pg": "^8.11.11",
    "@types/pg-copy-streams": "^1.2.5",
    "eslint": "^9.19.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  },
  "packageManager": "pnpm@9.5.0"
}
